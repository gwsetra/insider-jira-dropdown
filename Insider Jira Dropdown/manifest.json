 {
     "name": "Insider Jira Dropdown",
     "version": "1.1",
     "description": "Makes Jira Dropdown Searchable",
     "permissions": ["activeTab"],
     "content_scripts": [
     {
         "matches": ["https://winsider.atlassian.net/*"],
         "js": ["select2/jquery-3.4.1.min.js", "select2/select2.min.js", "convertor.js"],
         "css": ["select2/select2.min.css"],
         "run_at": "document_end"
     }],
     "icons":
     {
         "128": "images/128.png"
     },
     "manifest_version": 2
 }